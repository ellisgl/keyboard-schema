{
    "$schema" : "http://json-schema.org/draft-07/schema#",
    "$id"     : "https://raw.githubusercontent.com/ellisgl/keyboard-schema/release/position.schema.json",
    "title"   : "Unit setting.",
    "definitions" : {
        "Unit" : {
            "description" : "in = inches, mm = millimeters, u is unit size - based from keyboard.schema.json#uSize",
            "type"    : "string",
            "enum"    : [
                "in",
                "mm",
                "u"
            ]
        }
    }
}
